{
  "name": "@uplc/parser",
  "version": "0.1.0",
  "description": "UPLC parser and @harmoniclabs/uplc AST converter",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "build": "tsc",
    "test": "vitest run",
    "test:watch": "vitest",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@harmoniclabs/uplc": "^1.4.1"
  },
  "devDependencies": {
    "vitest": "^2.0.0",
    "typescript": "^5.3.3"
  }
}
